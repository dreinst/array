#include <iostream>
using namespace std;

int main() {
    int angka [4];
    int array =5;
    
    for (int i = 0; i < array; i++) {
        cout << "Masukkan bilangan ke-"<< i <<" : ";
        cin >> angka [i];    
    }

    int sum = 0;
    for (int i = 0; i < array; i++){
        sum += angka [i];
    }
    
     cout << "Hasil penjumlahan dari 5 bilangan tersebut adalah: " << sum << endl;
     
    return 0;
}
